  "scripts": {
    "analyze": "node analyze.js",
    "start": "node server.js",
    "start:api": "yarn run start",
    "start:react": "npx react-app-rewired start",
    "build": "export REACT_APP_build=$(git rev-parse --short HEAD) && npx react-app-rewired build",
    "build:nocheck": "yarn run build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "docker": "docker build -t metahkg/metahkg-web .",
    "deploy": "yarn run build && yarn run start",
    "dev": "yarn run deploy",
    "prepare": "husky install",
    "lint": "npx eslint --cache --fix src && (npx prettier --check src || npx prettier --write src)"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,jsx,tsx,ts}": "eslint --cache --fix",
    "*.{js,css,md,jsx,tsx,ts}": "prettier --write"
  }
}
